# Optimizaciones adicionales para producción
# Estas configuraciones ayudan a reducir la latencia con BD remota

# Pool optimizado para latencia
MYSQL_CONNECTION_LIMIT=15

# Configuración de logs
LOG_LEVEL=error
DISABLE_LOGIN_LOGS=true

# Nota: Las siguientes opciones no son compatibles con mysql2 y generan warnings:
# - MYSQL_CONNECT_TIMEOUT
# - MYSQL_ACQUIRE_TIMEOUT
# - MYSQL_QUERY_TIMEOUT
# - MYSQL_IDLE_TIMEOUT
# - MYSQL_COMPRESSION
# - MYSQL_CACHE_ENABLED
